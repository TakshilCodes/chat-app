{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",

    "strict": true,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true
  },
  "include": ["src"]
}
